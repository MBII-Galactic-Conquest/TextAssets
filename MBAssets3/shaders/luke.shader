// anh luke

models/players/luke_anh/poncho
{
	cull	disable
	{
		map models/players/luke_anh/poncho
		alphaFunc GE128
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		depthWrite
		rgbGen lightingDiffuse
	}
}

models/players/luke_anh/belt
{
	{
		map models/players/luke_anh/belt
		rgbGen lightingDiffuse
	}
	{
		map models/players/luke_anh/belt_s
		blendFunc GL_SRC_ALPHA GL_ONE
		alphaGen lightingSpecular
		detail
	}
}

models/players/luke_anh/hair
	{
		cull twosided
	{
		map models/players/luke_anh/hair
		rgbGen lightingDiffuse
	}
 	{
        		map models/players/luke_esb/LukeHair_spec
       		 blendFunc GL_SRC_ALPHA GL_ONE
      		  detail
       		 alphaGen lightingSpecular
	}
	}


