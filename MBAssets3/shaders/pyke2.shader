models/players/pyke2/torso_a
{
    {
        map models/players/pyke2/torso_a
        rgbGen lightingDiffuse
        //alphaFunc GE128
    }
    {
        map models/players/pyke2/torso_a_spec
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
	detail
    }
}

