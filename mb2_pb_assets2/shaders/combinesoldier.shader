

models/players/combinesoldier/combinesoldier
{
	cull	twosided
    {
        map models/players/combinesoldier/combinesoldier
        blendFunc GL_ONE GL_ZERO
        rgbGen lightingDiffuse
    }
    {
        map models/players/combinesoldier/combinesoldiersheet_mask
        blendFunc GL_SRC_ALPHA GL_ONE
        detail
        alphaGen lightingSpecular
    }
}

