models/players/DavyJonesy/Algaes
{
    cull disable
    {
        map models/players/DavyJonesy/Algaes
        alphaFunc GE128
        rgbGen lightingDiffuse
    }
    {
        map models/players/DavyJonesy/Algaes
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
        rgbGen lightingDiffuse
        detail
    }
}

