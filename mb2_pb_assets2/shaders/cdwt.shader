models/players/cdwt/feathers
{
	cull twosided
    {
        map models/players/cdwt/feathers
        rgbGen lightingDiffuse
    }
    {
        map models/players/cdwt/feathers
        blendFunc GL_SRC_ALPHA GL_ONE
        alphaGen lightingSpecular
	detail
    }
}

